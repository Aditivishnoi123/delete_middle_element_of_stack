
#include <iostream>
#include<bits/stdc++.h>

using namespace std;
void solve_the_stack_to_del_middle_element(stack<int> &s,int k){
    k=(k/2)+1;
    if(k==1){
        s.pop();
        return;
    }
    int temp=s.top();
    s.pop();
    solve_the_stack_to_del_middle_element(s,k-1);
    s.push(temp);
}
void middleelement(stack<int> &s,int k){
    if(s.size()==0){
        return;
    }
    k=s.size();
    
    solve_the_stack_to_del_middle_element(s,k);
}

int main()
{
    stack<int> s;
    s.push(1);
    s.push(2);
    s.push(3);
    s.push(4);
    s.push(5);
    middleelement(s,5);
    while(s.size()>0){
        cout<<s.top();
        s.pop();
    }
    return 0;
}
